{
    "name": "krn/php-cs-style",
    "description": "PHP CS Fixer config for M6Web components",
    "keywords": ["CS", "CodingStyle"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KRN",
            "email": "krn@krone.at",
            "homepage": "http://krone.at"
        }
    ],
    "require": {
        "friendsofphp/php-cs-fixer": "^3.1",
        "symfony/service-contracts": "^3.6",
        "symfony/translation-contracts": "^3.6",
        "symfony/cache-contracts": "^3.6",
        "symfony/deprecation-contracts": "^3.6",
        "symfony/event-dispatcher-contracts": "^3.6",
        "symfony/http-client-contracts": "^3.6",
        "symfony/console": "^6.2 || ^7.0",
        "symfony/event-dispatcher": "^6.2 || ^7.0",
        "symfony/filesystem": "^6.2 || ^7.0",
        "symfony/finder": "^6.2 || ^7.0",
        "symfony/options-resolver": "^6.2 || ^7.0",
        "symfony/process": "^6.2 || ^7.0",
        "symfony/stopwatch": "^6.2 || ^7.0",
        "symfony/string": "^6.2 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "KRN\\CS\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    }
}
